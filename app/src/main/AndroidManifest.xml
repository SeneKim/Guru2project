<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.example.guru2project">

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Guru2Project"
        tools:targetApi="31">
        <activity
            android:name=".login.preTest.testResultActivity"
            android:exported="false" />
        <activity
            android:name=".login.preTest.preTest3Activity"
            android:exported="false" />
        <activity
            android:name=".login.preTest.preTest2Activity"
            android:exported="false" />
        <activity
            android:name=".login.preTest.preTest1Activity"
            android:exported="false" />
        <activity
            android:name=".login.auth.signUpActivity"
            android:exported="false" />
        <activity
            android:name=".MainScreenActivity"
            android:exported="false" />
        <activity
            android:name=".login.auth.IntroActivity"
            android:exported="false" />
        <activity
            android:name=".login.SplashActivity"
            android:exported="true">

            <!-- 앱 실행 시 처음으로 실행 -->
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name=".MainActivity"
            android:exported="true" />
    </application>

</manifest>