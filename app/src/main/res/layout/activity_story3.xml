<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/stage3"
    tools:context=".story.Story3Activity">

    <TextView
        android:id="@+id/text3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="76dp"
        android:layout_marginEnd="20dp"
        android:gravity="center"
        android:text="새로운 도전을 위해 비건빌리지로 향하는데,,"
        android:textColor="@color/black"
        android:textSize="20dp"
        android:fontFamily="@font/notosanskr_bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/me3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="1dp"
        android:layout_marginBottom="199dp"
        android:background="@drawable/me3"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="@+id/friend" />

    <ImageView
        android:id="@+id/friend"
        android:layout_width="155dp"
        android:layout_height="95dp"
        android:layout_marginTop="50dp"
        android:background="@drawable/friend"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/me3" />



</androidx.constraintlayout.widget.ConstraintLayout>